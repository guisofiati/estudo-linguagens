Algoritmo "teste-formatacao-reais"

Var

numero1 : real
numero2 : real

Inicio

numero1 <- 25.67
numero2 <- 2.457578

escreval("*", numero1:8:1, "*")
escreval("*", numero1:4:1, "*")
escreval("*", numero1:4:2, "*")
escreval("*", numero1:6:2, "*")
escreval
escreval("*", numero2:8:6, "*")
escreval("*", numero2:8:5, "*")
escreval("*", numero2:8:4, "*")
escreval("*", numero2:2:5, "*")

Fimalgoritmo